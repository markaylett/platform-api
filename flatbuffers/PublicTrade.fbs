// {{namespace}}

table PublicTrade {
    source_ts: int64 (id: 0);
    source: string (id: 1);
    market: string (id: 2, required);
    feed_id: int32 (id: 3);
    trade_id: string (id: 4);
    flags: uint16 = 0 (id: 5);
